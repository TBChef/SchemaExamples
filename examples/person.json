{
    "$id": "https://github.com/TBChef/SchemaExamples/blob/main/examples/person.json",
    "$schema": "https://github.com/TBChef/SchemaExamples/blob/main/examples/person.json",
    "title": "Person",
    "type": "object",
    "properties": {
      "firstName": {
        "type": "string",
        "description": "The person's first name."
      },
      "lastName": {
        "type": "string",
        "description": "The person's last name."
      },
      "age": {
        "description": "Age in years which must be equal to or greater than zero.",
        "type": "integer",
        "minimum": 0
      }
    }
  }